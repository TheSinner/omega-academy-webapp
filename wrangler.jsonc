{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "omega-academy",
  "compatibility_date": "2025-10-26",
  "pages_build_output_dir": "./dist",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "omega-academy-db",
      "database_id": "local-db-id"
    }
  ],
  "r2_buckets": [
    {
      "binding": "IMAGES",
      "bucket_name": "omega-academy-images"
    }
  ]
}